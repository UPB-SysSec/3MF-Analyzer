    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
            "args": []
        },
        {
            "name": "Create Tests",
            "type": "python",
            "request": "launch",
            "module": "threemf_analyzer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "create",
                "-x",
                // "-m",
            ]
        },
        {
            "name": "Build Tests",
            "type": "python",
            "request": "launch",
            "module": "threemf_analyzer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "build",
            ]
        },
        {
            "name": "Run Tests",
            "type": "python",
            "request": "launch",
            "module": "threemf_analyzer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "run",
                "-p",
                "chitubox,cura,flashprint,fusion,ideamaker,lib3mf",
                // "meshmagic,meshmixer,office,paint3d,prusa,repetier",
                // "simplify,slic3r,superslicer,3dbuilder,3dviewer,zsuite",
            ]
        },
        {
            "name": "Gather Data",
            "type": "python",
            "request": "launch",
            "module": "threemf_analyzer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "gather",
            ]
        },
        {
            "name": "Start Server",
            "type": "python",
            "request": "launch",
            "module": "threemf_analyzer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "server",
            ]
        },
        {
            "name": "Evaluate Data",
            "type": "python",
            "request": "launch",
            "module": "threemf_analyzer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "evaluate",
                "-m",
                // "prusa",
            ]
        },
    ]
}